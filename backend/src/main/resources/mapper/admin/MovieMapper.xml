<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.x.backend.mapper.admin.MovieMapper">

    <insert id="createMove">
        INSERT INTO movie (name,director,actor , types, cover,year,language,rating,users,summary,video)
        VALUES (#{name}, #{director}, #{actor}, #{types}, #{cover}, #{year}, #{language}, #{rating}, #{users}, #{summary}, #{video})
    </insert>
</mapper>